{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Run example example with feature \"pillow\"",
			"type": "cargo",
			"command": "run",
			"args": ["--example", "example", "--features", "pillow"],
			"problemMatcher": ["$rustc"],
			"group": {
				"kind": "build",
				"isDefault": true
			}
		},
		{
			"label": "Run example",
			"type": "cargo",
			"command": "run",
			"args": ["--example", "example"],
			"problemMatcher": ["$rustc"],
			"group": {
				"kind": "build",
				"isDefault": false
			}
		}
	]
}
